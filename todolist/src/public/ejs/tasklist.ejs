<script type="text/template" id="unfListTmpl">
    <% for(var k in unfList){ %>
    <%
      if(unfList[k].fDate === "") {
        var fDate = "";
      }else{
        var fDate = Util.dateFormat(new Date(unfList[k].fDate), 'YYYY-MM-DD');
      }
    %>
    <div class="item <% if(Util.isPassedDate(unfList[k].fDate)){ %>passed<% } %>" taskId="<%=unfList[k].id%>" list-id="<%=unfList[k].listId%>" >
      <div class="l">
        <input type="checkbox" class="isFinished">
        <a href="javascript:;" class="name"><%=unfList[k].taskname%></a>
      </div>
      <div class="r">
        <div class="date"><%= fDate %></div>
      </div>
    </div>
    <% } %>
</script>
<script type="text/template" id="fListTmpl">
    <% for(var k in fList){ %>
    <%
      if(fList[k].fDate === "") {
        var fDate = "";
      }else{
        var fDate = Util.dateFormat(new Date(fList[k].fDate), 'YYYY-MM-DD');
      }
    %>
    <div class="item" taskId="<%=fList[k].id%>" list-id="<%=fList[k].listId%>" >
      <div class="l">
        <input type="checkbox" class="isFinished" checked >
        <div class="title">
            <a href="javascript:;" class="name"><%=fList[k].taskname%></a>
          <!--<span class="ago"><%=fList[k].ago%></span>-->
        </div>
      </div>
      <div class="r">
        <div class="date"><%= fDate %></div>
      </div>
    </div>
    <% } %>
</script>
